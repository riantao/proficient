{
	"$schema": "..\\..\\..\\node_modules\\nx\\schemas\\project-schema.json",
	"sourceRoot": "packages/libs/fixtures/src",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "@nrwl/js:tsc",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/packages/libs/fixtures",
				"main": "packages/libs/fixtures/src/index.ts",
				"tsConfig": "packages/libs/fixtures/tsconfig.lib.json",
				"assets": ["packages/libs/fixtures/*.md"]
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["packages/libs/fixtures/**/*.ts"]
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["coverage/packages/libs/fixtures"],
			"options": {
				"jestConfig": "packages/libs/fixtures/jest.config.ts",
				"passWithNoTests": true
			}
		}
	},
	"tags": [
		"type:util",
		"scope:internal",
		"platform:shared"
	]
}
