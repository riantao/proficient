{
  "$schema": "..\\..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "sourceRoot": "packages/libs/data-access/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/libs/data-access",
        "main": "packages/libs/data-access/src/index.ts",
        "tsConfig": "packages/libs/data-access/tsconfig.lib.json",
        "assets": ["packages/libs/data-access/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/libs/data-access/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/libs/data-access"],
      "options": {
        "jestConfig": "packages/libs/data-access/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": [
    "type:util",
    "scope:shared",
    "domain:client"
  ]
}
