{
	"$schema": "..\\..\\..\\node_modules\\nx\\schemas\\project-schema.json",
	"sourceRoot": "packages/libs/components/src",
	"projectType": "library",
	"tags": [],
	"targets": {
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["packages/libs/components/**/*.{ts,tsx,js,jsx}"]
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["coverage/packages/libs/components"],
			"options": {
				"jestConfig": "packages/libs/components/jest.config.ts",
				"passWithNoTests": true
			}
		},
		"storybook": {
			"executor": "@nrwl/storybook:storybook",
			"options": {
				"uiFramework": "@storybook/react",
				"port": 4400,
				"config": {
					"configFolder": "packages/libs/components/.storybook"
				}
			},
			"configurations": {
				"ci": {
					"quiet": true
				}
			}
		},
		"build-storybook": {
			"executor": "@nrwl/storybook:build",
			"outputs": ["{options.outputPath}"],
			"options": {
				"uiFramework": "@storybook/react",
				"outputPath": "dist/storybook/libs-components",
				"config": {
					"configFolder": "packages/libs/components/.storybook"
				}
			},
			"configurations": {
				"ci": {
					"quiet": true
				}
			}
		}
	}
}
